# Google Maps API Key (required)
GOOGLE_MAPS_API_KEY=your_api_key_here

# Model Configuration
MODEL_TYPE=segmentation           # 'bbox' or 'segmentation'
MODEL_PATH_SEG=./trained_model/best_seg.pt
MODEL_PATH_BBOX=./trained_model/best_bbox.pt

# Detection Settings
CONFIDENCE_THRESHOLD=0.15         # Lower = more detections
USE_SAHI=true                     # Sliced inference for better detection
SAHI_SLICE_SIZE=512
SAHI_OVERLAP_RATIO=0.3

# Image Settings
ZOOM_LEVEL=20                     # Max zoom for detail

# Paths
INPUT_FOLDER=./input_folder
OUTPUT_FOLDER=./prediction_files
ARTEFACTS_FOLDER=./artefacts